\section{Instruments and voice}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\subsection{Stringed instruments}

\begin{itemize}

\item Two types: bowed and plucked

\item Bowed: violin, cello, ...

\item Plucked: guitar, harp, ...

\end{itemize}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\subsection{Woodwind instruments}

\begin{itemize}

\item Two types: no reed, reed 

\item No reed: flute, recorder, ...

\item Reed: clarinet, bassoon, oboe

\item Clarinet: only instrument that produces only odd harmonics
(cylindrical bore, closed tube)

\end{itemize}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\subsection{Brass instruments}

\begin{itemize}

\item Two basic types: valves, slide

\item Valves: trumpet, french horn, tuba, ...

\item Slide: trombone

\item Depressing valves increase length of tube

\end{itemize}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\subsection{Piano}

\begin{itemize}

\item Piano-forte: soft, loud

\end{itemize}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\subsection{Percussion instruments}

\begin{itemize}

\item Two basic types: idiophones, membranophones

\item Idiophones: xylophone, chimes, triangle, cymbal, ...

\item Membranophones: drum, timpani

\end{itemize}

